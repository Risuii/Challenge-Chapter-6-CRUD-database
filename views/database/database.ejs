<html>
    <head>
        <link rel="stylesheet" type="text/css" href="/css/database.css" />
        <title>Database | All</title>
    </head>
    <body>
        <h1>Database</h1>
        <div class="new">
            <a href="/database/create">Mau Buat Data Baru ? Klik Disini !!!</a>
        </div>
        <% database.forEach((i) => { %>
            <div class="database">
                <div class="judul">
                    <div class="isiJudul">
                        <h1>
                            <%= i.username %>
                        </h1>
                    </div>
                </div>
                
                <br>


                <a href="/database/<%= i.id %>">
                    Lihat Data ? Klik Disini !
                </a>

                <br>
                <br>

                <a href="/database/update/<%= i.id %>">
                    Mau Update Data ? Klik Disini Aja :3
                </a>

                <br>
                <br>

                <a href="/database/delete/<%= i.id %>">
                    Mau Delete Data ? Klik Disini Aja :3
                </a>
            </div>
        <%}) %>
    </body>
</html>